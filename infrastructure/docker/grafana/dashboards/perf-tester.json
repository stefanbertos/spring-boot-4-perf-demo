{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Container Resources",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Container CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\".*perf-tester.*\", image!=\"\"}[$__rate_interval])",
          "legendFormat": "{{name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "fillOpacity": 20
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Container Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{name=~\".*perf-tester.*\", image!=\"\"}",
          "legendFormat": "{{name}} working set"
        },
        {
          "expr": "container_spec_memory_limit_bytes{name=~\".*perf-tester.*\", image!=\"\"} > 0",
          "legendFormat": "{{name}} limit"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "fillOpacity": 20
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Container Network I/O",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=~\".*perf-tester.*\", image!=\"\"}[$__rate_interval])",
          "legendFormat": "{{name}} rx"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=~\".*perf-tester.*\", image!=\"\"}[$__rate_interval])",
          "legendFormat": "{{name}} tx"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "fillOpacity": 20
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Container Filesystem I/O",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(container_fs_reads_bytes_total{name=~\".*perf-tester.*\", image!=\"\"}[$__rate_interval])",
          "legendFormat": "{{name}} read"
        },
        {
          "expr": "rate(container_fs_writes_bytes_total{name=~\".*perf-tester.*\", image!=\"\"}[$__rate_interval])",
          "legendFormat": "{{name}} write"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "fillOpacity": 20
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Application Metrics",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "collapsed": false
    },
    {
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"perf-tester\"}[$__rate_interval])",
          "legendFormat": "{{method}} {{uri}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "title": "HTTP Response Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_sum{job=\"perf-tester\"}[$__rate_interval]) / rate(http_server_requests_seconds_count{job=\"perf-tester\"}[$__rate_interval])",
          "legendFormat": "avg - {{method}} {{uri}}"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"perf-tester\"}[$__rate_interval]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_server_requests_seconds_bucket{job=\"perf-tester\"}[$__rate_interval]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "MQ E2E TPS",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(mq_e2e_latency_seconds_count{job=\"perf-tester\"}[$__rate_interval])",
          "legendFormat": "E2E TPS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "title": "MQ E2E Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(mq_e2e_latency_seconds_bucket{job=\"perf-tester\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mq_e2e_latency_seconds_bucket{job=\"perf-tester\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(mq_e2e_latency_seconds_bucket{job=\"perf-tester\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99"
        },
        {
          "expr": "rate(mq_e2e_latency_seconds_sum{job=\"perf-tester\"}[$__rate_interval]) / rate(mq_e2e_latency_seconds_count{job=\"perf-tester\"}[$__rate_interval])",
          "legendFormat": "avg"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Service Health",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 34 },
      "collapsed": false
    },
    {
      "title": "Service Health Status",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 35 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "health_mq_status{job=\"perf-tester\"}",
          "legendFormat": "IBM MQ"
        },
        {
          "expr": "health_kafka_status{job=\"perf-tester\"}",
          "legendFormat": "Kafka"
        },
        {
          "expr": "health_oracle_status{job=\"perf-tester\"}",
          "legendFormat": "Oracle"
        },
        {
          "expr": "health_redis_status{job=\"perf-tester\"}",
          "legendFormat": "Redis"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "DOWN", "color": "red" },
                "1": { "text": "UP", "color": "green" }
              }
            }
          ],
          "unit": "short"
        },
        "overrides": []
      }
    },
    {
      "title": "Health Check Ping Duration",
      "type": "timeseries",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 35 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(health_ping_duration_seconds_sum{job=\"perf-tester\"}[$__rate_interval]) / rate(health_ping_duration_seconds_count{job=\"perf-tester\"}[$__rate_interval])",
          "legendFormat": "{{service}} avg"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "JVM",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "collapsed": false
    },
    {
      "title": "JVM Heap Memory",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"perf-tester\", area=\"heap\"})",
          "legendFormat": "used"
        },
        {
          "expr": "sum(jvm_memory_committed_bytes{job=\"perf-tester\", area=\"heap\"})",
          "legendFormat": "committed"
        },
        {
          "expr": "sum(jvm_memory_max_bytes{job=\"perf-tester\", area=\"heap\"}) > 0",
          "legendFormat": "max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "title": "CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "process_cpu_usage{job=\"perf-tester\"}",
          "legendFormat": "process CPU"
        },
        {
          "expr": "system_cpu_usage{job=\"perf-tester\"}",
          "legendFormat": "system CPU"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "title": "GC Overhead",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 50 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "jvm_gc_overhead{job=\"perf-tester\"}",
          "legendFormat": "GC overhead"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "max": 1
        },
        "overrides": []
      }
    },
    {
      "title": "JVM Threads",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 50 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "jvm_threads_live_threads{job=\"perf-tester\"}",
          "legendFormat": "live"
        },
        {
          "expr": "jvm_threads_daemon_threads{job=\"perf-tester\"}",
          "legendFormat": "daemon"
        },
        {
          "expr": "jvm_threads_peak_threads{job=\"perf-tester\"}",
          "legendFormat": "peak"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "title": "Perf Tester",
  "uid": "perf-tester",
  "version": 1
}
